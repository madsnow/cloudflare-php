{
  "name": "cloudflare/sdk",
  "description": "PHP binding for v4 of the Cloudflare Client API.",
  "type": "library",
  "require": {
    "php": ">=7.2.5",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0.1",
    "psr/http-message": "^2.0"
  },
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Junade Ali",
      "email": "junade@cloudflare.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cloudflare\\API\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
        "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
